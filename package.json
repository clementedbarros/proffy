{
  "name": "proffy",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",  
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },  
  "devDependencies": {
    "commitizen": "^4.1.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
